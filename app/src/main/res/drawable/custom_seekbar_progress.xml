<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#70ffffff" />
            <size android:height="2.0dip" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <solid android:color="@color/white" />
                <size android:height="2.0dip" />
                <corners
                    android:topLeftRadius="2.0dip"
                    android:topRightRadius="2.0dip"
                    android:bottomLeftRadius="2.0dip"
                    android:bottomRightRadius="2.0dip" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="#ffff5468" />
                <size android:height="4.0dip" />
            </shape>
        </clip>
    </item>
</layer-list>