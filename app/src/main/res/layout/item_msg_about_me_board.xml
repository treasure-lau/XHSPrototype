<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:tag="layout/item_msg_about_me_board_0"
    android:background="@drawable/common_white_to_gray"
    android:paddingLeft="@dimen/dimension_15"
    android:paddingTop="@dimen/dimension_15"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.xingin.xhs.widget.AvatarImageView
        android:id="@+id/iv_avatar"
        android:tag="binding_1"
        android:layout_width="35.0dip"
        android:layout_height="35.0dip"
        android:layout_marginTop="15.0dip"
        android:layout_centerVertical="true"
        app:roundAsCircle="true" />

    <com.xingin.xhs.widget.XYImageView
        android:id="@+id/iv_one"
        android:tag="binding_2"
        android:layout_width="35.0dip"
        android:layout_height="35.0dip"
        android:layout_marginTop="20.0dip"
        android:layout_marginRight="@dimen/dimension_15"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        app:viewAspectRatio="1.0"
        app:placeholderImage="@color/background"
        app:failureImage="@color/background" />

    <com.xingin.xhs.widget.XYImageView
        android:id="@+id/iv_two"
        android:layout_width="35.0dip"
        android:layout_height="35.0dip"
        android:layout_marginTop="20.0dip"
        android:layout_marginRight="2.0dip"
        android:layout_toLeftOf="@id/iv_one"
        android:layout_centerVertical="true"
        app:viewAspectRatio="1.0"
        app:placeholderImage="@color/background"
        app:failureImage="@color/background" />

    <com.xingin.xhs.view.FlowLayout
        android:orientation="vertical"
        android:id="@+id/tv_msg_about_me_title"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimension_15"
        android:layout_marginRight="@dimen/dimension_15"
        android:layout_toLeftOf="@id/iv_two"
        android:layout_toRightOf="@id/iv_avatar"
        android:layout_centerVertical="true">

        <TextView
            android:textSize="@dimen/dimension_12"
            android:id="@+id/tv_msg_about_me_nickname"
            android:paddingRight="3.0dip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:textSize="@dimen/dimension_12"
            android:id="@+id/tv_msg_about_me_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </com.xingin.xhs.view.FlowLayout>

    <View
        android:tag="binding_3"
        android:layout_width="fill_parent"
        android:layout_height="1.0px"
        android:layout_marginTop="@dimen/dimension_15"
        android:layout_below="@id/tv_msg_about_me_title"
        android:layout_alignLeft="@id/tv_msg_about_me_title"
        android:layout_alignParentBottom="true" />

    <View
        android:tag="binding_4"
        android:layout_width="fill_parent"
        android:layout_height="1.0px"
        android:layout_marginTop="@dimen/dimension_15"
        android:layout_below="@id/tv_msg_about_me_title"
        android:layout_alignParentBottom="true" />
</RelativeLayout>